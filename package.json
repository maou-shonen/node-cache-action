{
  "name": "node-cache-action",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "ncc build src/main.ts -o dist/main --source-map && ncc build src/cache-save.ts -o dist/cache-save --source-map",
    "test": "vitest run",
    "lint": "biome check --write .",
    "typecheck": "tsc --noEmit",
    "check": "pnpm lint && pnpm typecheck && pnpm test && pnpm build"
  },
  "dependencies": {
    "@actions/cache": "^5.0.1",
    "@actions/core": "^2.0.1",
    "@actions/exec": "^2.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.0.3",
    "@vercel/ncc": "^0.38.4",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.27.0"
}
